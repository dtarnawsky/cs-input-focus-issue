{
  "name": "Mobile_Retire",
  "version": "0.0.20",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "angular-build-info && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --ignore-path .gitignore --ignore-pattern node_modules --write src",
    "install:ci": "npm install && (npm list || true)",
    "build:ci": "npm run install:ci && npm run lint && npm run build && pwd && ls -lrt",
    "test:ci": "ng test --karma-config karma.pipeline.conf.js --code-coverage --watch false",
    "cleanup": "rm -rf package-lock.json && rm -rf node_modules && rm -rf www",
    "prebuild": "npm --no-git-tag-version version patch",
    "addAndroid": "rm -rf android && npx cap add android",
    "addIos": "rm -rf ios && npx cap add ios",
    "capSync": "npx cap sync",
    "openAndroid": "npm run build && npm run addAndroid && npm run capSync && npx cap open android",
    "openIos": "npm run build && npm run addIos && npm run capSync && npx cap open ios",
    "mobileBuilds": "npm run openAndroid && npm run openIos",
    "livereload:android": "npx ionic cap run android -l --port=8200",
    "livereload:ios": "npx ionic cap run ios -l --port=8100"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@capacitor/android": "3.1.2",
    "@capacitor/app": "^1.0.2",
    "@capacitor/core": "^3.1.2",
    "@capacitor/device": "^1.0.2",
    "@capacitor/ios": "3.4.3",
    "@capacitor/keyboard": "^1.2.1",
    "@capacitor/status-bar": "^1.0.2",
    "@ionic-enterprise/identity-vault": "^4.3.3",
    "@ionic-native/core": "^5.32.1",
    "@ionic-native/http": "^5.33.0",
    "@ionic-native/in-app-browser": "^5.35.0",
    "@ionic/angular": "^5.9.3",
    "@thiagoprz/ionic-currency-mask": "^0.0.13",
    "angular-build-info": "^2.0.1",
    "cordova-plugin-advanced-http": "^3.1.0",
    "cordova-plugin-file": "^6.0.2",
    "rxjs": "~6.6.0",
    "swiper": "^8.0.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular-eslint/builder": "2.0.2",
    "@angular/cli": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/compiler-cli": "~12.2.16",
    "@capacitor/cli": "^3.1.2",
    "@ionic/angular-toolkit": "^3.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.15",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-plugin-html": "^6.2.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sauce-launcher": "^4.3.6",
    "lint-staged": "^10.5.4",
    "prettier": "1.19.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.{ts,js,scss,css,html}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "description": "An Ionic project"
}
